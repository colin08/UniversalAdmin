<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1,maximum-scale=1,user-scalable=no" />
    <link href="~/Assets/mui/css/mui.min.css" rel="stylesheet" />
    <title>@ViewData["Title"]</title>

    @RenderSection("htmlhead", required: false)

</head>
<body>
    <!--MUI 头部导航栏 -->

    @RenderSection("muiheader", required: false)

    <!-- MUI核心中间部分 -->
    <div class="mui-content">
        @RenderBody()
    </div>

    <!--MUI 底部内容-->
    @RenderSection("muifooter", required: false)

</body>
    @RenderSection("htmlfoot", required: false)
</html>
