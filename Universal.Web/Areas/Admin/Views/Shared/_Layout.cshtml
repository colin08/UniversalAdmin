<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewData["Title"]</title>

    @Styles.Render("~/Content/admin/css")

    @RenderSection("head",required:false)
</head>

<body class="gray-bg">
    <div class="wrapper wrapper-content animated fadeInRight">
        <div class="ibox">
            <div class="ibox-content">
                <div class="row">
                    
                    @RenderSection("nav")
                    
                </div>
                <hr />

                @RenderBody()

            </div>
            
            @{ 
                if (ViewData.ContainsKey("ExecutionTime"))
                {
                    <text>
                    <div class="pull-left" style="margin: 10px 0 0 15px;">
                    time：@ViewData["ExecutionTime"] ms   
                    <div>
                    </text>
                }
            }

        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/layer")
    @Scripts.Render("~/bundles/tools")

    @RenderSection("foot",required:false)

</body>
</html>